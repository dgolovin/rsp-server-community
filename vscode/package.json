{
  "name": "vscode-community-server-connector",
  "displayName": "Community Server Connectors",
  "description": "Connects vscode with open source community-driven servers and runtimes.",
  "version": "0.22.11",
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "redhat",
  "author": "Red Hat",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-developer/rsp-server-community.git"
  },
  "bugs": "https://github.com/redhat-developer/rsp-server-community.git",
  "engines": {
    "vscode": "^1.30.0"
  },
  "activationEvents": [
    "onView:servers",
    "onView:serversAB"
  ],
  "categories": [
    "Other"
  ],
  "keywords": [
    "server",
    "runtimes",
    "adapters"
  ],
  "icon": "images/community.png",
  "main": "./out/src/extension",
  "scripts": {
    "package": "node ./build/package.js",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./out/test/test-runner.js",
    "update-deps": "node_modules/.bin/ncu --upgrade --loglevel verbose --packageFile package.json && npm update",
    "coverage:upload": "codecov -f coverage/coverage-final.json",
    "build": "npm run compile && npm run package"
  },
  "extensionDependencies": [
    "redhat.vscode-rsp-ui"
  ],
  "devDependencies": {
    "@types/glob": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.35",
    "@types/sinon": "^9.0.4",
    "@types/vscode": "^1.46.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.7.0",
    "decache": "^4.6.0",
    "decompress": "^4.2.1",
    "download": "^7.1.0",
    "glob": "^7.1.6",
    "istanbul": "^0.4.5",
    "mocha": "^8.0.1",
    "mocha-jenkins-reporter": "^0.4.3",
    "remap-istanbul": "^0.13.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "tslint": "^6.1.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.5",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "expand-home-dir": "0.0.3",
    "find-java-home": "^0.2.0",
    "fs-extra": "7.0.0",
    "path": "0.12.7",
    "path-exists": "^3.0.0",
    "portfinder": "^1.0.20",
    "rsp-client": "^0.15.0",
    "tcp-port-used": "^1.0.1",
    "vscode-server-connector-api": "0.1.5",
    "wait-on": "^3.0.1"
  }
}
